<h1>Log In</h1>
<%= form_with(url: login_path, local: true) do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email, name: 'session[email]', placeholder: "Email" %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password, name: 'session[password]', placeholder: "Password" %>
  </div>
  <div>
    <%= form.submit "Login" %>
  </div>
<% end %>
<%= link_to 'Sign up', signup_path %>
