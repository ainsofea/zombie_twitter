<p>Sign up</p>
<%= form_with model: @user, local: true, html: { autocomplete: "off" } do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name, placeholder: "Name" %>
  </div>
  <div>
    <%= form.label :graveyard %>
    <%= form.text_area :graveyard, placeholder: "Graveyard" %>
  </div>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email, placeholder: "Email" %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password, placeholder: "Password" %>
  </div>
  <div>
    <%= form.submit "Register" %>
  </div>
<% end %>
<%= link_to 'Login', login_path %>
